<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="codeone.com.br.mobile_gym_pass.features.box.fragment.BoxFragment">

    <androidx.cardview.widget.CardView
        app:cardElevation="6dp"
        app:cardPreventCornerOverlap="false"
        app:cardUseCompatPadding="true"
        tools:cardCornerRadius="2dp"
        tools:cardElevation="6dp"
        android:foreground="?attr/selectableItemBackground"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/tTxtAmbiente"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="6dp"
            android:background="@mipmap/ic_fundo_device_big_foreground_favorite"
            android:fontFamily="@font/abril_fatface_latin"
            android:gravity="start|center_horizontal"
            android:padding="5dp"
            android:textColor="@color/black"
            android:textSize="13sp" />

    </androidx.cardview.widget.CardView>



    <androidx.cardview.widget.CardView
        android:layout_marginTop="-10dp"
        app:cardElevation="6dp"
        app:cardPreventCornerOverlap="false"
        app:cardUseCompatPadding="true"
        tools:cardCornerRadius="2dp"
        tools:cardElevation="6dp"
        android:foreground="?attr/selectableItemBackground"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/tNmInfoImportante"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="6dp"
            android:background="@mipmap/ic_fundo_device_big_foreground_favorite"
            android:fontFamily="@font/abril_fatface_latin"
            android:gravity="start|center_horizontal"
            android:padding="5dp"
            android:textColor="@color/black"
            android:textSize="13sp" />

    </androidx.cardview.widget.CardView>


</LinearLayout>